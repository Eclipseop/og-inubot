<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.inubot.model.Script" table="bone_scripts">
        <id name="id" type="int" column="script_id">
            <generator class="native"/>
        </id>
        <property name="name" type="string" column="script_name"/>
    </class>
</hibernate-mapping>