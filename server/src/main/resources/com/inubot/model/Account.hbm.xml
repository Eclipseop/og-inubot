<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.inubot.model.Account" table="core_members">
        <id name="id" type="int" column="member_id">
            <generator class="native"/>
        </id>
        <property name="username" type="string" column="name"/>
        <property name="password" type="string" column="members_pass_hash"/>
    </class>
</hibernate-mapping>